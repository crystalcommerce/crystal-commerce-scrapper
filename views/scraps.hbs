                        {{#if disabled}}
                        <span>Enable</span>
                        {{else}}
                        <span>Disable</span>
                        {{/if}}
                    </span>
                </a> --}}
                <a href="scraps/delete/{{_id}}" class="btn btn-info"><span uk-icon="trash"></span></a>
            </td>
        </tr>
